<% method = locals[:method] -%>
<% inout_data = method.parameters.last -%>
    public static func <%= SwiftHelper.format_name(method.name) %>(<%= SwiftHelper.parameters(method.parameters[0...-1]) %>) -> Data {
        var result = Data(repeating: 0, count: <%= inout_data.type.size %>)
        result.withUnsafeMutableBytes { ptr in
            <%- arguments = SwiftHelper.arguments(method.parameters[0...-1]) + ['ptr'] -%>
            TW<%= @entity.name %><%= method.name %>(<%= arguments.join(', ') %>)
        }
        return result
    }
