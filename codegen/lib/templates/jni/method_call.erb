<%- method = locals[:method] -%>
<%- if method.parameters.count > 1 -%>
TW<%= entity.name %><%= method.name %>(<%= method.entity.is_struct ? '*' : '' %>instance, <%= JNIHelper.arguments(method.parameters.drop(1)) %>);
<%- else -%>
TW<%= entity.name %><%= method.name %>(<%= method.entity.is_struct ? '*' : '' %>instance);
<%- end -%>