<%  method = locals[:method] -%>
    public func <%= SwiftHelper.format_name(method.name) %>(<%= SwiftHelper.parameters(method.parameters.drop(1)) %>) -> String {
<%= render('swift/parameter_access.erb', { parameters: method.parameters.drop(1) }) -%>
<%  arguments = ['rawValue'] + SwiftHelper.arguments(method.parameters.drop(1)) -%>
        return String.fromTWString(TW<%= @entity.name %><%= method.name %>(<%= arguments.join(', ') %>))
    }
