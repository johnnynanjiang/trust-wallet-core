<%-
method = locals[:method]
inout_data = method.parameters.last
 -%>
    public static func <%= Swift.format_name(method.name) %>(<%= Swift.parameters(method.parameters[0...-1]) %>) -> Data {
        var result = Data(repeating: 0, count: <%= inout_data.type.size %>)
        result.withUnsafeMutableBytes { ptr in
        <%- if method.parameters.count > 1 -%>
            TW<%= @entity.name %><%= method.name %>(<%= Swift.arguments(method.parameters[0...-1]) %>, ptr)
        <%- else -%>
            TW<%= @entity.name %><%= method.name %>(ptr)
        <%- end -%>
        }
        return result
    }
