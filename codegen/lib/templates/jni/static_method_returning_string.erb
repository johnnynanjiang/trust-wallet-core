<%  method = locals[:method] -%>
<%= render('jni/static_method_returning_string_prototype.erb', { method: method }) %> {
<%= render('jni/parameter_access.erb', { parameters: method.parameters }) -%>
    jstring result = TWStringJString(TW<%= entity.name %><%= method.name %>(<%= JNIHelper.arguments(method.parameters) %>), env);
<%= render('jni/parameter_release.erb', { parameters: method.parameters }) -%>
    return result;
}
