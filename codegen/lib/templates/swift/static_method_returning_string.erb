<% method = locals[:method] -%>
    public static func <%= SwiftHelper.format_name(method.name) %>(<%= SwiftHelper.parameters(method.parameters) %>) -> String {
        <%- arguments = SwiftHelper.arguments(method.parameters) -%>
        return String.fromTWString(TW<%= @entity.name %><%= method.name %>(<%= arguments.join(', ') %>))
    }
